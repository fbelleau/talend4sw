<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Bio2RDF"
    RELEASE_DATE="20130919"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Semantic Web/Virtuoso</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:JDBC"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      
	/>
	
   <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="10"
      SHOW="false"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
	
	<PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tVirtuosoConnection"
      NUM_ROW="10"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
      DYNAMIC_SETTINGS="true"
    />
    
	
    <PARAMETER
      NAME="URL"
      FIELD="TEXT"
      NUM_ROW="15"
      REPOSITORY_VALUE="URL"
      REQUIRED="true" 
     SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	>
      <DEFAULT>"localhost:1111"</DEFAULT>
    </PARAMETER>
	
	<PARAMETER
      NAME="DATABASE"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="DATABASE"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	>
	<DEFAULT>"DB"</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	>
      <DEFAULT>"dba"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true" 
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	>
      <DEFAULT>"dba"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="35"
    >
      <TABLE READONLY="false">
        <COLUMN NAME="subject" TYPE="id_String" />
        <COLUMN NAME="predicate" TYPE="id_String" />
        <COLUMN NAME="object" TYPE="id_String" />
      </TABLE>
    </PARAMETER>

    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="55"
      REQUIRED="true"
    >
      <DEFAULT>"construct {?s ?p ?o .}  where {?s ?p ?o .}"</DEFAULT>
    </PARAMETER>
		
   </PARAMETERS>

   <ADVANCED_PARAMETERS>
	<PARAMETER
      	NAME="ENCODING"
      	FIELD="ENCODING_TYPE"
      	NUM_ROW="10"
      	REQUIRED="true"
      	SHOW="false"
    >
      	<DEFAULT>"ISO-8859-15"</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="USE_CURSOR" FIELD="CHECK" NUM_ROW="20">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="CURSOR_SIZE" FIELD="TEXT" NUM_ROW="20"
			SHOW_IF="(USE_CURSOR == 'true')">
			<DEFAULT>1000</DEFAULT>
	</PARAMETER>

	
	
	<PARAMETER NAME="TRIM_ALL_COLUMN" FIELD="CHECK" NUM_ROW="30">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="TRIM_COLUMN" FIELD="TABLE" NUM_ROW="40"
		NB_LINES="5" SHOW_IF="TRIM_ALL_COLUMN == 'false'">
		<ITEMS BASED_ON_SCHEMA="true">
			<ITEM NAME="TRIM" FIELD="CHECK"></ITEM>
		</ITEMS>
	</PARAMETER>		
   </ADVANCED_PARAMETERS>

   <CODEGENERATION>
	   <IMPORTS>
	      <IMPORT NAME="virtjdbc3" MODULE="virtjdbc3.jar" UrlPath="http://www.openlinksw.com/dataspace/doc/dav/wiki/Main/VOSDownload/virtjdbc3.jar" REQUIRED="true" />
	   </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW"/>
   </RETURNS>
</COMPONENT>